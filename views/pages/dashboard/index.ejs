                          <th>Waktu Mulai</th>
                          <th class="text-center">Actions</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% table.forEach(function (o) { %>
                        <tr>
                          <td><%= o.name.toUpperCase() %></td>
                          <td>
                            <% const trans = transaction.find((t) =>
                            t._id.toString() === o.transactionId); %> <% if
                            (o.status === "kosong") { %>
                            <div class="d-flex align-items-center gap-3">
                              <a href="javascript:void(0);">
                                <span class="d-block"></span>
                              </a>
                            </div>
                            <% } else { %>
                            <div class="d-flex align-items-center gap-3">
                              <a href="javascript:void(0);">
                                <span class="d-block"><%= trans.nama %></span>
                                <span class="fs-12 d-block fw-normal text-muted"
                                  ><%= trans.nowa %></span
                                >
                              </a>
                            </div>
                            <% } %>
                          </td>
                          <td>
                            <span class="badge bg-gray-200 text-dark"
                              ><%= o.status == "kosong" ? "none" :
                              trans.paket.toUpperCase() %></span
                            >
                          </td>
                          <td><%= o.status.toUpperCase() %></td>
                          <td>
                            <%= o.status === "kosong" ? "" :
                            trans.startTime?.toLocaleString() %>
                          </td>

                          <td>
                            <div class="d-flex justify-content-center gap-3">
                              <% if (o.status !== "kosong") { if(trans.status
                              !== "bermain") {%>
                              <button
                                onclick="confirmAhref(`/transaction/start/<%= trans._id %>`)"
                                class="btn btn-success"
                              >
                                Start
                              </button>
                              <% }else{ %>
                              <button
                                onclick="confirmAhref(`/transaction/end/<%= trans._id %>`)"
                                class="btn btn-danger"
                              >
                                Selesai
                              </button>
                              <% }} %>
                            </div>
                          </td>
                        </tr>
                        <% }); %>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
