                  <div class="table-responsive">
                    <table class="table table-hover mb-0">
                      <thead>
                        <tr class="border-b">
                          <th scope="row">Nama</th>
                          <th>Type</th>
                          <th>Waktu</th>
                          <th class="text-center">Actions</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% transaction.forEach(function(t) { %> <% const table =
                        t.table.split('-'); %> <% if (table[0] === 'wait') { %>
                        <tr>
                          <td>
                            <div class="d-flex align-items-center gap-3">
                              <a href="javascript:void(0);">
                                <span class="d-block"><%= t.nama %></span>
                                <span class="fs-12 d-block fw-normal text-muted"
                                  ><%= t.nowa %></span
                                >
                              </a>
                            </div>
                          </td>
                          <td>
                            <span class="badge bg-gray-200 text-dark">
                              <%= table[1] %>
                            </span>
                          </td>
                          <td><%= t.createdAt.toLocaleString() %></td>
                          <td>
                            <div class="d-flex justify-content-center gap-3">
                              <a
                                href="/transaction/delete/<%= t._id %>"
                                class="btn btn-danger"
                                >Hapus</a
                              >
                              <button
                                class="btn btn-primary"
                                data-bs-toggle="modal"
                                onclick="updateValue('<%= JSON.stringify(t) %>')"
                                data-bs-target="#orderUpdate"
                              >
                                Lanjutkan
                              </button>
                            </div>
                          </td>
                        </tr>
                        <% } %> <% }); %>
                      </tbody>
                    </table>
